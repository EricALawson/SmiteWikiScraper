{
  "name": "smite-wiki-scraper",
  "version": "1.0.0",
  "description": "webscraper to read smite wiki and save item and god stats to a file",
  "main": "src/index.js",
  "dependencies": {
    "@types/mongodb": "^3.6.7",
    "@types/node": "^14.14.25",
    "@types/puppeteer": "^5.4.3",
    "chromedriver": "^87.0.7",
    "mongodb": "^3.6.4",
    "puppeteer": "^5.5.0",
    "smite-timeline": "github:EricALawson/smite-timeline-react#master",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "test-one-god": "tsc && node -r source-map-support/register build/src/entrypoints.js testgods",
    "test-one-item": "tsc && node -r source-map-support/register build/src/entrypoints.js testitems",
    "start": "tsc && node -r source-map-support/register build/src/entrypoints.js gods",
    "mongo": "tsc && node -r source-map-support/register build/src/index.js gods"
  },
